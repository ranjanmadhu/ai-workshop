# Common Gitignore

# Ignore operating system files
.DS_Store
Thumbs.db

# Ignore editor files
*.swp
*.swo
*.bak

# Ignore build output
build/
dist/
*.exe
*.dll
*.so
*.dylib

# Ignore dependencies
node_modules/
vendor/

# Ignore logs and temporary files
*.log
*.tmp
*.temp

# Ignore environment-specific files
.env
.env.local
.env.*.local
.env.*.production
.env.production.local

# Ignore sensitive information
secrets.yml
secrets.json
secrets.env

package-lock.json